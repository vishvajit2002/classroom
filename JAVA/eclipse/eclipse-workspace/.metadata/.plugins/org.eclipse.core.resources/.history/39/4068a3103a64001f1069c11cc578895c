package Arrayeg;
public class Jaggedarra
{
public static void main(String[] args)
{
	int arr[]=new int[2][];
	arr[0]=new int[3];
	arr[2]=new int[3];
	int count=0;
	for(int i=0;i<arr.length;i++)
		for(int j=0; j<arr[i].length;j++)
			arr[i][j]=count++;
	System.out.println("Displaying the contents of Jagged array");

	
}
}
